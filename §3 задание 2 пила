//чуток не правильно, пила началась не по восходящей, как в задании, а по нисходящей...
//но мозг прокачивается ёпсиль-мопсиль...
##
uses graphabc;
var (x,y) := (25,25);
for var i:=0 to 8 do begin
Line(x,y,x+25,y+25);
x+=25;
y+=25;
Line(x,y,x+25,y-25);
x+=25;
y-=25;
end;
/////////////////////
// уфф, сделал:
##
uses graphabc;
var (x,y) := (100,100);
for var i:=1 to 8 do begin
Line(x,y,x+25,y-25);
x+=25;
y-=25;
Line(x,y,x+25,y+25);
x+=25;
y+=25;
end;{for i}
