uses graphabc;
begin
  for var x := 0 to 3 do 
    for var y := 0 to 255 do     
      SetPixel(x, y, RGB(0, 0, 0));   
  for var x := 0 to 255 do 
    for var y := 0 to 3 do     
      SetPixel(x, y, RGB(0, 0, 0));  
  for var x := 0 to 255 do 
    for var y := 252 to 255 do     
      SetPixel(x,y, RGB(0, 0, 0));  
  for var x := 252 to 255 do 
    for var y := 0 to 255 do     
      SetPixel(x,y, RGB(0, 0, 0));  
end.
